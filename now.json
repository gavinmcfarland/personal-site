{
	"version": 2,
	"github": {
		"enabled": false
	},
	"builds": [{
		"src": "dist/client/**",
		"use": "@now/static"
	}, {
		"src": "dist/server/main.js",
		"use": "@now/node-server"
	}],
	"routes": [{
			"src": "/static/(.*)",
			"dest": "dist/client/$1",
			"methods": ["GET"]
		},
		{
			"src": "/(.*)",
			"dest": "dist/server/main.js",
			"methods": ["GET"]
		}
	]
}
