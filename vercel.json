{
	"version": 2,
	"github": {
		"silent": true
	},
	"builds": [{
		"src": "public/client/**",
		"use": "@vercel/static"
	}, {
		"src": "public/server/main.js",
		"use": "@vercel/node"
	}],
	"alias": ["www.gavinmcfarland.co.uk"],
	"routes": [{
			"src": "/static/(.*)",
			"dest": "public/client/$1",
			"methods": ["GET"]
		},
		{
			"src": "/(.*)",
			"dest": "public/server/main.js",
			"methods": ["GET"]
		},
		{
			"src": "/(.*)",
			"status": 301,
			"headers": { "Location": "https://gavinmcfarland.co.uk/$1" }
		}
	]
}
